<% form_for([@product]) do |f| %>
<%= render "shared/error_messages", :target => @product %>
<p>
        <%= f.label :name %> <br />
        <%= f.text_field :name %> <br />
        <%= f.label :description %> <br />
        <%= f.text_area :description %> <br />
</p>
<p>
        <%= f.submit 'Post' %>
</p>
<% end %>

